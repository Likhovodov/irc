{
    "globals": {
        "socket.io-client": true
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-classes-per-file": 0,
        "no-unused-vars": 0,
        "no-alert": 0,
        "no-use-before-define": 0
    }
}